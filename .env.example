# Node環境
NODE_ENV=development

# Stationhead設定
STATIONHEAD_URL=https://www.stationhead.com
STATIONHEAD_LOGIN_URL=https://www.stationhead.com/on/sign-in
STATIONHEAD_EMAIL=your-email@example.com
STATIONHEAD_PASSWORD=your-password

# Spotify設定
SPOTIFY_EMAIL=your-spotify-email@example.com
SPOTIFY_PASSWORD=your-spotify-password

# 自動配信設定
SHOW_NAME=My Daily Show
PLAYLIST_NAME=My Playlist
DURATION=60

# Slack通知設定
SLACK_WEBHOOK_URL=
SLACK_NOTIFICATIONS_ENABLED=true

# スケジューラー設定
# チェック間隔（ミリ秒）
SCHEDULER_CHECK_INTERVAL_MS=60000

# ブラウザ設定
BROWSER_HEADLESS=true
BROWSER_SLOW_MO=0
BROWSER_TIMEOUT=30000

# ログ設定
LOG_LEVEL=info
